{
  "name": "fundraisely-client",
  "private": true,
  "version": "0.1.0",
  "type": "module",
 "scripts": {
  "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
  "server:dev": "npm run dev --prefix server",
  "client:dev": "npm run dev --prefix client",

  "install": "npm install --prefix client && npm install --prefix server",

  "build": "npm run build --prefix client",
  "start": "npm run build && npm run start --prefix server"
}
,
  "dependencies": {
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.20.1",
    "socket.io-client": "^4.8.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^5.4.2"
  }
}

